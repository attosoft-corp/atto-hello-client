{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "atto-hello-client.businnes.svc"
    },
    "cloud": {
      "config": {
        "enable": false
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": false,
      "shouldFetchRegistry": false
    }
  },
  "info": {
    "description": "Hello from the Development config file"
  },
  "swagger": {
    "title": "Hello Client",
    "version": "v1",
    "description": "Application test to say hello"
  },
  "atto-joke-config": {
    "Url": "http://localhost:5000/"
  },
  "hystrix": {
    "command": {
      "default": {
        "execution": {
          "isolation": {
            "thread": {
              "timeoutInMilliseconds": 61000
            }
          }
        }
      }
    }
  }

}