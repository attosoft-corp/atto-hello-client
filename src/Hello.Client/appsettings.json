{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "atto-hello-client.businnes.svc"
    },
    "cloud": {
      "config": {
        "uri": "http://atto-config-server.common.svc:8080",
        "validate_certifates": false
      }
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://atto-eureka-server.common.svc:8080/eureka/",
      "shouldFetchRegistry": false,
      "ValidateCertificates":  false
    },
    "instance": {
      "port": 8080
    }
  }
}

