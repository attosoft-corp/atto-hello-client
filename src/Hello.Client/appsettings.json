{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Pivotal": "Debug",
      "Steeltoe": "Debug"
    }
  },
  "spring": {
    "application": {
      "name": "hello-client"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certifates": false
      }
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": false,
      "ValidateCertificates": false
    },
    "instance": {
      "port": 5010
    }
  },
  "hystrix": {
    "stream": {
      "validate_certificates": false
    },
    "command": {
      "JokeCommand": {
        "threadPoolKeyOverride": "JokeServiceTPool"
      }
    }
  }
}