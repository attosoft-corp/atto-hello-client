{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Pivotal": "Debug",
      "Steeltoe": "Debug"
    }
  },
  "spring": { 
    "application": {
      "name": "atto-hello-client.businnes.svc"
    },
    "cloud": {
      "config": {
        "uri": "http://atto-config-server.common.svc:8080",
        "validate_certifates": false
      }
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://atto-eureka-server.common.svc:8080/eureka/"
    },
    "instance": {
      "hostname": "atto-eureka-server.common.svc",
      "port": 8080
    }
  },
  "hystrix": {
    "stream": {
      "validate_certificates": false
    },
    "command": {
      "JokeCommand": {
        "threadPoolKeyOverride": "JokeServiceTPool"
      }
    }
  }
}